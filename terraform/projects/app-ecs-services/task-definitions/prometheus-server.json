[
  {
    "name": "prometheus",
    "image": "prom/prometheus",
    "cpu": 2048,
    "memory": 8192,
    "essential": true,
    "portMappings": [
      {
        "containerPort": 9090,
        "hostPort": 9090
      }
    ],
    "mountPoints": [
      {
        "sourceVolume": "prometheus-etc",
        "containerPath": "/etc/prometheus"
      }
    ]
  }
]
